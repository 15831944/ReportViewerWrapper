      <Query>
        <DataSourceName>HBD_CAFM_V3</DataSourceName>
        <CommandText>-- SELECT SO_ApertureDWG FROM T_Standort -- S0541_0000
-- SELECT GBI_ApertureDWG FROM T_GebaeudeIMMO -- S0550_0000
-- SELECT GSS_ApertureDWG FROM T_Geschoss -- G00886-UG01_0000


-- DECLARE @in_aperturedwg varchar(50) 
-- SET @in_aperturedwg = 'G00020-OG02_0000' 
-- SET @in_aperturedwg = 'S0691_0000' 
-- SET @in_aperturedwg = 'G00234-EZ00_0000' 


-- DECLARE @in_stylizer nvarchar(200) 
-- -- SET @in_stylizer = N'REM Bodenbelag' 
-- -- -- SET @in_stylizer = N'' 
-- -- -- SET @in_stylizer = NULL 


SELECT TOP 1 
	 T_Standort.SO_UID
	,T_Standort.SO_Nr 
	,T_Standort.SO_Name AS SO_Bezeichnung
	,T_Standort.SO_Prfx
	
	--,T_Standort.SO_IsSchnittstelleWP
	
	--,T_AP_Ref_Freigabetyp.FGT_UID
	--,T_AP_Ref_Freigabetyp.FGT_Code

	--,T_AP_Ref_Freigabetyp.FGT_Kurz_DE
	--,T_AP_Ref_Freigabetyp.FGT_Kurz_FR
	--,T_AP_Ref_Freigabetyp.FGT_Kurz_IT
	--,T_AP_Ref_Freigabetyp.FGT_Kurz_EN

	--,T_AP_Ref_Freigabetyp.FGT_Lang_DE
	--,T_AP_Ref_Freigabetyp.FGT_Lang_FR
	--,T_AP_Ref_Freigabetyp.FGT_Lang_IT
	--,T_AP_Ref_Freigabetyp.FGT_Lang_EN
	
	
	,T_GebaeudeIMMO.GBI_Nr AS GB_Nr 
	,T_GebaeudeIMMO.GBI_Name  AS GB_Bezeichnung 
	
	
	,T_Geschoss.GSS_Nr AS GS_Nr 
	,T_Geschoss.GSS_Bezeichnung AS GS_Bezeichnung 
	,T_Geschoss.GSS_IsAGF AS GS_IsAussengeschoss 
	,T_Ref_Geschosstyp.GSST_UID 
	
	,T_Ref_Geschosstyp.GSST_Kurz AS GST_Kurz_DE 
	,T_Ref_Geschosstyp.GSST_Kurz AS GST_Kurz_FR 
	,T_Ref_Geschosstyp.GSST_Kurz AS GST_Kurz_IT 
	,T_Ref_Geschosstyp.GSST_Kurz AS GST_Kurz_EN 

	,T_Ref_Geschosstyp.GSST_Lang AS GST_Lang_DE
	,T_Ref_Geschosstyp.GSST_Lang AS GST_Lang_FR
	,T_Ref_Geschosstyp.GSST_Lang AS GST_Lang_IT
	,T_Ref_Geschosstyp.GSST_Lang AS GST_Lang_EN
	
	
	,T_Standort.SO_ApertureDWG AS ZO_SODWG_ApertureDWG
	,T_Standort.SO_ObjectID AS ZO_SODWG_ApertureObjID
	
	,T_GebaeudeIMMO.GBI_ApertureDWG AS ZO_GBDWG_ApertureDWG 
	,T_GebaeudeIMMO.GBI_ObjectID AS ZO_GBDWG_ApertureObjID

	,T_Geschoss.GSS_ApertureDWG AS ZO_GSDWG_ApertureDWG 
	,T_Geschoss.GSS_ObjectID AS ZO_GSDWG_ApertureObjID 
	
	,T_Geschoss.GSS_FlaecheGraphisch AS ZO_GSFlaeche_Flaeche 
	
	,SO_Prfx + RIGHT('0000' + CAST(T_Standort.SO_Nr AS varchar(10)), 4) AS RPT_SO_DisplayText 
	,GBI_Prfx + RIGHT('0000' + CAST(T_GebaeudeIMMO.GBI_Nr AS varchar(10)), 4) AS RPT_GB_DisplayText 
	,ISNULL(T_GebaeudeIMMO.GBI_PlzCAFM + ' ', '') + T_GebaeudeIMMO.GBI_OrtCAFM AS RPT_PLZ_Ort 
	
	,COALESCE(T_Geschoss.GSS_ApertureDWG, T_GebaeudeIMMO.GBI_ApertureDWG, T_Standort.SO_ApertureDWG) + '.dwg' AS RPT_OBJ_DWG 
	,T_GebaeudeIMMO.GBI_StrasseCAFM + ISNULL(' ' + T_GebaeudeIMMO.GBI_HausnrCAFM, '') AS RPT_Strasse 
	 
	,ZO_RMRSP_GueltigVon AS RPT_GueltigVon 
	,ZO_RMRSP_GueltigBis AS RPT_GueltigBis 
FROM T_Standort 

--LEFT JOIN T_AP_Ref_Freigabetyp 
--	ON T_AP_Ref_Freigabetyp.FGT_Code = T_Standort.SO_IsSchnittstelleWP 
--	AND T_AP_Ref_Freigabetyp.FGT_Status = 1 

LEFT JOIN T_GebaeudeIMMO 
	ON T_GebaeudeIMMO.GBI_SO_UID = T_Standort.SO_UID 
	AND T_GebaeudeIMMO.GBI_Status = 1 
	AND CONVERT(char(8), {fn now()}, 112) BETWEEN CONVERT(char(8), T_GebaeudeIMMO.GBI_DatumVon, 112) AND T_GebaeudeIMMO.GBI_DatumBis 
	
LEFT JOIN T_Geschoss 
	ON T_Geschoss.GSS_GBI_UID = T_GebaeudeIMMO.GBI_UID 
	AND T_Geschoss.GSS_Status = 1 
	AND CONVERT(char(8), {fn now()}, 112) BETWEEN CONVERT(char(8), T_Geschoss.GSS_DatumVon, 112) AND T_Geschoss.GSS_DatumBis 
	AND EXISTS(SELECT GSS_UID FROM T_Geschoss WHERE GSS_ApertureDWG = @in_aperturedwg) 
	
LEFT JOIN T_Ref_Geschosstyp 
	ON T_Ref_Geschosstyp.GSST_UID = T_Geschoss.GSS_GSST_UID 
	AND T_Ref_Geschosstyp.GSST_Status = 1 
	
LEFT JOIN T_ZO_AP_SHOW_Geschoss_Reinigungsplanung 
	ON T_ZO_AP_SHOW_Geschoss_Reinigungsplanung.ZO_GSSRSP_GSS_UID = T_Geschoss.GSS_UID 
	AND SUBSTRING(@in_stylizer, 1, 3) = 'REM' 
	
LEFT JOIN T_ZO_REM_AP_Standort_Planung 
	ON T_ZO_REM_AP_Standort_Planung.ZO_RMRSP_Nr = T_ZO_AP_SHOW_Geschoss_Reinigungsplanung.ZO_GSSRSP_REMSP_Nr 
	AND T_ZO_REM_AP_Standort_Planung.ZO_RMRSP_Status = 1 
	
WHERE (1=1) 
AND T_Standort.SO_Status = 1 
AND CONVERT(char(8), {fn now()}, 112) BETWEEN CONVERT(char(8), T_Standort.SO_DatumVon, 112) AND T_Standort.SO_DatumBis 
AND 
( 
	T_Geschoss.GSS_ApertureDWG = @in_aperturedwg 
	OR 
	T_GebaeudeIMMO.GBI_ApertureDWG = @in_aperturedwg 
	OR 
	T_Standort.SO_ApertureDWG = @in_aperturedwg 
) 

ORDER BY GSS_IsAGF</CommandText>
